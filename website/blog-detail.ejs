
<head>
	<% include ./includes/head-meta-tag.ejs %>
	<% include ./includes/head-link-tag.ejs %>
	<% include ./includes/head-script-tag.ejs %>
</head>

<body>
	<% include ./includes/body-no-script-tag.ejs %>
	<div id="nsg-taxi-wrapper">
		<div id="nsg-taxi-page">
			<% include ./includes/header.ejs %>
			<div id="nsg-taxi-blog-section" class="nsg-taxi-section-gray">
				<div class="container">
					<div class="row row-bottom-padded-md">
						<div class="col-lg-12 col-md-12 col-sm-12">
							<div class="nsg-taxi-blog animate-box">
								<div class="blog-text" style="height: auto;">
									<div class="prod-title">
										<h3><%= blog_heading %></h3>
										<span class="posted_by">
											<%= moment(created_at).format('MMMM Do YYYY, h:mm:ss a') %>
										</span>
										<p><%- blog_description %></p>
										<p><%- blog_content %></p>
										<p>
										<img class="img-responsive blog-details-image" src="<%= blog_image %>" alt="<%= blog_image_name %>">
										</p>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<% include ./includes/footer.ejs %>
		</div>
	</div>
	<% include ./includes/footer-script.ejs %>
</body>