					  /* Float four columns side by side */
.column_mobile_routes {
  float: left;
  width: 25%;
  padding: 0 10px;
  
}
.h2_mobile_routes {
  font-size: 0 rem;
}

/* Remove extra left and right margins, due to padding */
.row_mobile_routes {margin: 0 -5px;}

/* Clear floats after the columns */
.row_mobile_routes:after {
  content: "";
  display: table;
  clear: both;
}
.to_mobile_routes{
  font-size: 2rem;
    font-weight: bold;
	margin: 0px;
}
.route_mobile_routes{
  font-size: 20px;
    font-weight: bold;
    text-decoration: underline;
	color: #f78536;
	margin: 0 0 10px 0;
}
.link_mobile_routes {
  color: #f78536;
}



/* Responsive columns */
@media screen and (max-width: 600px) {
  .column_mobile_routes {
    width: 50%;
    display: block;
    margin-bottom: 20px;
    padding: 0 20px;
  }
}

@media screen and (min-width: 0px) and (max-width: 1800px) {
  .row_mobile_routes {
    display: none;
  } /* show it on large screen */
}
@media screen and (min-width: 0px) and (max-width: 500px) {
  .row_mobile_routes {
    display: block;
  } /* hide it small screens */
}

/* Style the counter cards */
.card_mobile_routes {
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
  padding: 16px;
  text-align: center;
  background-color: white;
  border-radius: 20px;
}

</style>

